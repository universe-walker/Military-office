app:
  datasource:
    main:
      driver-class-name: org.postgresql.Driver
      jdbc-url: jdbc:postgresql://localhost:5432/military_office
      username:
      password:
      pool-size: 30

spring:
  jpa:
    properties:
      hibernate:
        show_sql: true
        format_sql: true
  flyway:
    baseline-on-migrate: true

logging:
  level:
    org:
      hibernate:
        type: trace